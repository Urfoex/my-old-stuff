####### Compile

.obj/main.o: main.cpp .ui/wormhole.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.ui/wormhole.h: wormhole.ui 
	$(UIC) wormhole.ui -o .ui/wormhole.h

.ui/wormhole.cpp: .ui/wormhole.h wormhole.ui wormhole.ui.h 
	$(UIC) wormhole.ui -i wormhole.h -o .ui/wormhole.cpp

.obj/wormhole.o: .ui/wormhole.cpp wormhole.ui.h \
		.ui/wormhole.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/wormhole.o .ui/wormhole.cpp

.obj/moc_wormhole.o: .moc/moc_wormhole.cpp  .ui/wormhole.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_wormhole.o .moc/moc_wormhole.cpp

.moc/moc_wormhole.cpp: $(MOC) .ui/wormhole.h
	$(MOC) .ui/wormhole.h -o .moc/moc_wormhole.cpp

.obj/qmake_image_collection.o: .ui/qmake_image_collection.cpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/qmake_image_collection.o .ui/qmake_image_collection.cpp

.ui/qmake_image_collection.cpp: images/filenew \
		images/fileopen \
		images/filesave \
		images/print \
		images/undo \
		images/redo \
		images/editcut \
		images/editcopy \
		images/editpaste \
		images/searchfind
	$(UIC)  -embed WormHole images/filenew images/fileopen images/filesave images/print images/undo images/redo images/editcut images/editcopy images/editpaste images/searchfind -o .ui/qmake_image_collection.cpp

